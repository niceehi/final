    printf("           @@@@@@@@@@@@@\n");
    printf("        @@@@@@@@@@@@@@@@@@@\n");
    printf("      @@@@@@@@@@@@@@@@@@@@@@@\n");
    printf("     @@@@@@@@@@@@@@@@@@@@@@@@@\n");
    printf("    @@@@@@@@@@@@@@@@@@@@@@@@@@@  <-- Messy hair\n");
    printf("   @@@@@@@@@@@@@@@@@@@@@@@@@@@@@\n");
    printf("   @@@@@@@@@@@@@@@@@@@@@@@@@@@@@\n");
    printf("   @@@   ________________   @@@\n");
    printf("   @@   /                \\   @@\n");
    printf("   @@  |    ---    ---    |  @@\n");
    printf("   @   |   ( o )  ( o )   |   @\n");
    printf("  @@   |        __        |   @@\n");
    printf("  @@   |       /  \\       |   @@\n");
    printf("   @    \\      \\__/      /    @\n");
    printf("    @    \\    ______    /    @\n");
    printf("     @@   \\  |      |  /   @@\n");
    printf("      @@@  \\__________/  @@@\n");
    printf("         @@@@  |  |  @@@@\n");
    printf("        @@@@@--|  |--@@@@@\n");
    printf("       /      /    \\      \\\n");
    printf("      /      /      \\      \\  <-- White shirt\n");
    printf("     /______/        \\______\\\n");

